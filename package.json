{
  "name": "react-tailblocks",
  "version": "0.1.0",
  "description": "Use mertJF's ready-to-use Tailwind CSS blocks as a reusable react library.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uay/react-tailblocks.git"
  },
  "keywords": [
    "uay",
    "react",
    "tailwind",
    "tailblocks",
    "mertJF"
  ],
  "author": "uay LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uay/react-tailblocks/issues"
  },
  "homepage": "https://github.com/uay/react-tailblocks#readme",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "tailwindcss": "^1.8.10"
  },
  "scripts": {
    "build:tailwind": "tailwindcss build src/tailwind.css -o src/tailwind.output.css",
    "prestart": "npm run build:tailwind",
    "prebuild": "npm run build:tailwind",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prop-types": "^15.7.2",
    "tailwindcss-dark-mode": "^1.1.6"
  }
}
